{
  "expo": {
    "name": "Hexpresso",
    "slug": "hexpresso",
    "version": "1.0.1",
    "versionCode": 2,
    "orientation": "portrait",
    "entryPoint": "./index.tsx",
    "newArchEnabled": true,
    "backgroundColor": "#d1d1d1",
    "scheme": "hexpresso",
    "experiments": {
      "typedRoutes": true
    },
    "plugins": [
      "@react-native-google-signin/google-signin",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "backgroundColor": "#d1d1d1",
          "dark": {
            "backgroundColor": "#d1d1d1"
          },
          "light": {
            "backgroundColor": "#d1d1d1"
          },
          "imageWidth": 256
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/logo_notification.png",
          "backgroundColor": "#d1d1d1",
          "defaultChannel": "default",
          "enableBackgroundRemoteNotifications": false
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Hexpresso uygulaması konumunuzu kullanarak size daha iyi hizmet sunmak istiyor."
        }
      ],
      [
        "react-native-fbsdk-next",
        {
          "appID": "1347955562982110",
          "clientToken": "69b436333e6e48d8c44c585c3ee9373f",
          "displayName": "Hexpresso"
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-2363017162977044~8550998696"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Hexpresso uygulaması fotoğrafınızı kullanarak size daha iyi hizmet sunmak istiyor."
        }
      ]
    ],
    "android": {
      "package": "com.faruktutkus.hexpresso",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png"
      },
      "googleServicesFile": "./google-services.json",
      "edgeToEdgeEnabled": true,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAZruxpLeabIOgxb2_Ky6eXhwYLDbhZIGE"
        }
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.INTERNET",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "ios": {
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Hexpresso uygulaması konumunuzu kullanarak size daha iyi hizmet sunmak istiyor.",
        "NSLocationAlwaysUsageDescription": "Hexpresso uygulaması konumunuzu kullanarak size daha iyi hizmet sunmak istiyor.",
        "SKAdNetworkItems": [
          {
            "SKAdNetworkIdentifier": "v9wttpbfk9.skadnetwork"
          },
          {
            "SKAdNetworkIdentifier": "n38lu8286q.skadnetwork"
          }
        ]
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "userInterfaceStyle": "automatic",
    "extra": {
      "eas": {
        "projectId": "b231d42f-2e5c-48fa-8336-d214c129b92e"
      }
    }
  }
}
